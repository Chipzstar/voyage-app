	"scripts": {
		"start": "nx serve",
		"build": "nx build",
		"test": "nx test",
		"sass:shippers": "sass --watch apps/shipper-dashboard/scss:apps/shipper-dashboard/styles",
		"sass:carriers": "sass --watch apps/carrier-dashboard/scss:apps/carrier-dashboard/styles",
		"nx:shipper:serve": "doppler run -p shipper-dashboard -c dev -- nx serve shipper-dashboard",
		"nx:shipper:build": "nx build shipper-dashboard",
		"nx:shipper:serve:verbose": "doppler run -p shipper-dashboard -c dev -- nx serve shipper-dashboard --verbose",
		"nx:shipper:export": "nx export shipper-dashboard",
		"nx:backend:serve": "doppler run -p voyage-backend -c dev -- nx serve backend",
		"nx:shipper:build:prod": "nx build shipper-dashboard --prod",
		"nx:shipper:build:prod:verbose": "nx build shipper-dashboard --prod --verbose",
		"nx:shipper:serve:local": "doppler run -p shipper-dashboard -c dev -- nx serve shipper-dashboard --prod",
		"nx:shipper:serve:prod": "nx serve shipper-dashboard --prod --hostname 0.0.0.0",
		"nx:carrier:serve": "doppler run -p carrier-dashboard -c dev -- nx serve carrier-dashboard",
		"nx:carrier:build": "nx build carrier-dashboard",
		"nx:carrier:serve:local": "doppler run -p carrier-dashboard -c dev -- nx serve carrier-dashboard --prod",
		"nx:carrier:serve:prod": "nx serve carrier-dashboard --prod --hostname 0.0.0.0",
		"nx:carrier:build:prod:verbose": "nx build carrier-dashboard --prod --verbose",
		"nx:cache-clean": "nx clear-cache",
		"prisma:generate": "npx prismerge -i prismerge.json && npx prisma generate"
	},
